vec4
fastuidraw_gl_frag_main(in uint shader_data_offset)
{
  float alpha;

  if(FASTUIDRAW_PAINTER_STROKE_STROKING_PASS_VALUE == fastuidraw_stroke_aa_pass)
    {
      float dd, q;

      q = 1.0 - abs(fastuidraw_stroking_on_boundary);
      dd = fwidth(q);
      alpha = min(1.0, q / dd);
    }
  else
    {
      alpha = 1.0;
    }

  return vec4(1.0, 1.0, 1.0, alpha);
}
